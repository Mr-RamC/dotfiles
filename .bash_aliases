# interactive operation
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'

# grep
alias grep='grep --color'
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'

# ls
alias ls='ls -hF --color=auto'
alias ll='ls -Al'
alias l=ls

# git
alias g=git
alias gg='git st'
alias gk='gitk&'
alias gka='gitk --all&'

# python
alias py=python
alias py3=python3
alias ve=virtualenv

# gradle
alias gr=gradle

# make
alias mk=make

# vagrant
alias vg=vagrant

# docker
alias dk=docker
